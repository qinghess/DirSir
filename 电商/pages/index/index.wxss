/**index.wxss**/
.header{
  width:100%;
  height:230rpx;
  background-color: rgb(240, 59, 27);
  display: flex;
  justify-content: center;
  align-items: center;
}
/* 搜索框 */
.search_box{
  width:93%;
  height:80rpx;
  background-color: #fff;
  border-radius: 30rpx;
  display: flex;
  margin-top: 90rpx;
}
.search_left{
  display: flex;
  flex:1;
  align-items: center;
}
.search_left icon{
  padding-left:30rpx;
  padding-right: 20rpx;
}
.search_btn{
  width:130rpx;
  height:80rpx;
  border-radius: 30rpx;
  background-color: rgb(240, 59, 27);
  color: #fff;
  font-weight: bold;
  text-align: center;
  line-height: 80rpx;
}
/* 页面主体部分 main */
.main{
  background-color: #e6e3e3;
  display: flex;
  justify-content: center;
  width:100%;
  height:100vh;
}
.main_one{
  width:93%;
  height:500rpx;
  background-color: #fff;
  border-radius: 30rpx;
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
}
.one_box{
  width: 130rpx;
  height:180rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  font-weight: bold;
  margin: 23rpx;
}
.one_img{
  width:130rpx;
  height:130rpx;
  border-radius: 50rpx;
}
.one_img image{
  width:130rpx;
  height:130rpx;
  border-radius: 50rpx;
}